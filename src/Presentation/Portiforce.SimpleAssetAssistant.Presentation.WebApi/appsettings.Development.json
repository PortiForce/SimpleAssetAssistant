{
  "ConnectionStrings": {
    "AssetAssistantSQLDb": "{from-user-secrets}"
  },
  "PlatformUsers": {
    "SQLPlatformUser": {
      "Login": "{from-configs}",
      "Password": "{from-configs}"
    }
  },
  "GoogleClientSettings": {
    "ClientId": "{from-user-secrets}"
  },
  "JwtSettings": {
    "Secret": "{from-user-secrets}",
    "Issuer": "https://api.portiforce.ai",
    "Audience": "https://portiforce.ai",
    "ExpiryMinutes": "15",
    "RefreshTokenDurationHours": "168"
  },
  "TokenHashingOptions": {
    "Pepper": "{from-user-secrets}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
